{
    "Serilog": {
        "Using": [ "Serilog.Sinks.File", "Serilog.Sinks.Console" ],
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft": "Warning",
                "Microsoft.Hosting.Lifetime": "Information",
                "System.Net.Http.HttpClient": "Error"

            }
        },
        "WriteTo": [
            {
                "Name": "File",
                "Args": {
                    "path": "./logs/log-.log",
                    "retainedFileCountLimit": 3,
                    "fileSizeLimitBytes": 52428800,
                    "rollingInterval": "Day",
                    "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] [{ApplicationName}] {Scope} {Message:lj}{NewLine}{Exception}"
                }
            }
        ]
    },
    "CORSOrigins": [],
    "GoogleDriveStorageProviderConfig": {
        "ClientId": "204276950334-atgm0fb9c4vl1vcogjfbvhb8s70se2r4.apps.googleusercontent.com",
        "RedirectUri": "http://localhost:5353/Auth/Google/AuthFinish"
    },
    "DropboxStorageProviderConfig": {
        "AppKey": "rnojgy4c8e2kohr",
        "RedirectUri": "http://localhost:5353/Auth/Dropbox/AuthFinish"
    },
    "GameSyncWorkerConfig": {
        "LoopDelayTimeSpan": "0.00:30:00"
    },
    "SyncTaskWorkerConfig": {
        "LoopDelayTimeSpan": "0.00:05:00"
    }
}