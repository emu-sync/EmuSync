    "RedirectUri": "http://localhost:5353/Auth/Google/AuthFinish"
  },
  "DropboxStorageProviderConfig": {
    "AppKey": "rnojgy4c8e2kohr",
    "RedirectUri": "http://localhost:5353/Auth/Dropbox/AuthFinish"
  },
  "OneDriveStorageProviderConfig": {
    "ClientId": "8b618adc-b743-4c17-b5a6-b0bdb9d15694",
    "RedirectUri": "http://localhost:5353/Auth/Microsoft/AuthFinish"
  },
  "GameSyncWorkerConfig": {
    "LoopDelayTimeSpan": "0.00:02:00"
  }
